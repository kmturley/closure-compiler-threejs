{
  "name": "closure-compiler-threejs",
  "version": "1.0.0",
  "description": "Example Three.js project compiled using Closure Compiler",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:compiler": "google-closure-compiler --flagfile ./compiler.conf",
    "start": "webpack-dev-server --open",
    "start:dist": "http-server ./dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch"
  },
  "keywords": [
    "Three.js",
    "Tween.js",
    "Closure",
    "Compiler"
  ],
  "author": "kmturley",
  "license": "ISC",
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "closure-webpack-plugin": "^2.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "es6-tween": "^5.5.10",
    "google-closure-compiler": "^20200204.0.0",
    "http-server": "^0.12.1",
    "three": "^0.113.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
